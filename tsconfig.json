{
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": false,
    "lib": ["esnext", "", "ESNext"]
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsc", "src/**/*.vue"]
}
